{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WorkflowEngine": {
    "UpdaterEnabled": true,
    "UpdaterStartDelay": 10,
    "UpdaterSchedule": "0/30 * * * * ?",
    "WorkflowDirectory": "workflows"
  }
}
